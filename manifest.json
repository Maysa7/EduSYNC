{
  "manifest_version": 3,
  "name": "EduS.Y.N.C.",
  "version": "1.0",
  "description": "Sync assignments from multiple LMS platforms to Google Calendar.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*"
  ],
  "oauth2": {
    "client_id": "1057854422312-kp6n7oso1pbihbntqp0dl4ol1nefh1d6.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "openid",
      "https://www.googleapis.com/auth/calendar"
    ]
  }
}
